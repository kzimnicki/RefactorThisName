<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >

    <g:HTMLPanel styleName="row">
        <div class="span10 well">



            <div id="myCarousel" class="carousel slide">
                <!-- Carousel items -->

                <div class="carousel-inner">
                    <div class="item">
                        <img src="img/got2.png" alt=""/>
                    </div>
                    <div class="item">
                        <img src="img/thm.png" alt=""/>
                    </div>
                    <div class="active item">
                        <img src="img/himym.png" alt=""/>
                    </div>
                </div>
                <!-- Carousel nav -->
                <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
                <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
            </div>

             <br />
            <div style="text-align: center;">
                <g:Button ui:field="start" styleName="btn btn-success btn-large big-button">Get Started</g:Button>
                <g:Button ui:field="more" styleName="btn btn-large big-button">Learn More</g:Button>
            </div>

        </div>


    </g:HTMLPanel>

</ui:UiBinder> 